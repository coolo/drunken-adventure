   box {
    <-LG_KNOB_INNER_RADIUS/2-LG_CORNER_SPACE, LG_WALL_WIDTH/2, -LG_CORNER_SPACE-LG_E>,
    <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -LG_WALL_WIDTH/2, LG_KNOB_INNER_RADIUS>
   }
   intersection {
    box {
     <-LG_KNOB_INNER_RADIUS/2+LG_E, LG_WALL_WIDTH/2+LG_E, LG_PLATE_HEIGHT+LG_E>,
     <-LG_BRICK_WIDTH/2-LG_E, -LG_WALL_WIDTH/2-LG_E, LG_KNOB_INNER_RADIUS>
    }
    merge {
     cylinder {
      <-LG_KNOB_RADIUS, LG_WALL_WIDTH/2, LG_KNOB_INNER_RADIUS>,
      <-LG_KNOB_RADIUS, -LG_WALL_WIDTH/2, LG_KNOB_INNER_RADIUS>,
      LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE
     }
     cylinder {
      <-LG_KNOB_RADIUS, LG_WALL_WIDTH/2-LG_CORNER_SPACE, LG_KNOB_INNER_RADIUS>,
      <-LG_KNOB_RADIUS, -LG_WALL_WIDTH/2+LG_CORNER_SPACE, LG_KNOB_INNER_RADIUS>,
      LG_KNOB_INNER_RADIUS
     }
     torus {
      LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      translate <-LG_KNOB_RADIUS, LG_WALL_WIDTH/2-LG_CORNER_SPACE, LG_KNOB_INNER_RADIUS>
     }
     torus {
      LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE,
      LG_CORNER_SPACE
      translate <-LG_KNOB_RADIUS, -LG_WALL_WIDTH/2+LG_CORNER_SPACE, LG_KNOB_INNER_RADIUS>
     }
    }
   }
   #if ((HOLDER = -2) | (HOLDER = 2))
    translate <0, HOLDER*(LG_BRICK_WIDTH-LG_WALL_WIDTH/4), 0>
   #else
    translate <0, HOLDER*LG_BRICK_WIDTH, 0>
   #end
  } 
  #declare HOLDER = HOLDER + 1;
 #end
 difference {
  merge {
   box {
    <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
    <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -2*LG_BRICK_WIDTH+LG_CORNER_SPACE, -LG_PLATE_HEIGHT>
   }
   box {
    <LG_BRICK_WIDTH/2, 2*LG_BRICK_WIDTH-LG_CORNER_SPACE, -LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2, -2*LG_BRICK_WIDTH+LG_CORNER_SPACE, -LG_PLATE_HEIGHT+LG_CORNER_SPACE>
   }
   box {
    <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 2*LG_BRICK_WIDTH, -LG_CORNER_SPACE>,
    <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, -2*LG_BRICK_WIDTH, -LG_PLATE_HEIGHT+LG_CORNER_SPACE>
   }
  }
  box {
   <LG_BRICK_WIDTH/2-LG_WALL_WIDTH, 2*LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_TOP_HEIGHT>,
   <-LG_BRICK_WIDTH/2+LG_WALL_WIDTH, -2*LG_BRICK_WIDTH+LG_WALL_WIDTH, -LG_PLATE_HEIGHT-LG_E>
  }
 }
 object {
  lg_plate_column_clear
  translate <0, -LG_BRICK_WIDTH, -LG_PLATE_HEIGHT>
 }
 object {
  lg_plate_column_clear
  translate <0, 0, -LG_PLATE_HEIGHT>
 }
 object {
  lg_plate_column_clear
  translate <0, LG_BRICK_WIDTH, -LG_PLATE_HEIGHT>
 }
}

