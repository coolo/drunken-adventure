   }
   box {
    <0, 2*LG_BRICK_WIDTH-LG_CORNER_SPACE, 0>,
    <-LG_WALL_WIDTH+LG_CORNER_SPACE, -2*LG_BRICK_WIDTH+LG_CORNER_SPACE, -(LG_PLATE_HEIGHT-LG_CORNER_SPACE)/cos(lg_angle)>
   }
 
   rotate <0, -lg_angle*180/pi, 0>
   translate <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, -LG_CORNER_SPACE>
  }
  box {
   <0, 2*LG_BRICK_WIDTH, -LG_PLATE_HEIGHT>,
   <1, -2*LG_BRICK_WIDTH, -2*LG_PLATE_HEIGHT>
  }
 }
 cylinder {
  <0, 2*LG_BRICK_WIDTH-LG_CORNER_SPACE, -(LG_PLATE_HEIGHT-2*LG_CORNER_SPACE)/cos(lg_angle)>,
  <0, -2*LG_BRICK_WIDTH+LG_CORNER_SPACE, -(LG_PLATE_HEIGHT-2*LG_CORNER_SPACE)/cos(lg_angle)>,
  LG_CORNER_SPACE
  rotate <0, -lg_angle*180/pi, 0>
  translate <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, 0, -LG_CORNER_SPACE>
 }
 box {
  <1.5*LG_BRICK_WIDTH-LG_CORNER_SPACE, 2*LG_BRICK_WIDTH-LG_CORNER_SPACE, -8*LG_PLATE_HEIGHT>,
  <-0.5*LG_BRICK_WIDTH+LG_CORNER_SPACE, -2*LG_BRICK_WIDTH+LG_CORNER_SPACE, -8*LG_PLATE_HEIGHT-LG_TOP_HEIGHT>
 }
