  }
  box {
   <LG_CORNER_SPACE, -LG_WALL_WIDTH-LG_E, LG_BRICK_HEIGHT/cos(lg_angle*pi/180)>,
   <-LG_BRICK_WIDTH, LG_E, 0>
   rotate <0,-lg_angle, 0>
   translate <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, LG_BRICK_WIDTH, -LG_BRICK_HEIGHT-LG_CORNER_SPACE>
  }
 }
 intersection {
  box {
   <LG_BRICK_WIDTH/2-LG_CORNER_SPACE, LG_BRICK_WIDTH, -LG_BRICK_HEIGHT-LG_CORNER_SPACE-LG_E>,
   <-LG_BRICK_WIDTH/2+LG_CORNER_SPACE, LG_BRICK_WIDTH-LG_WALL_WIDTH, -LG_KNOB_INNER_RADIUS+(LG_KNOB_INNER_RADIUS-LG_CORNER_SPACE)*sin(lg_angle*pi/180)+LG_CORNER_SPACE*sin(lg_angle*pi/180)>
  }
  box {
   <0, -LG_WALL_WIDTH-LG_E, LG_BRICK_HEIGHT/cos(lg_angle*pi/180)>,
