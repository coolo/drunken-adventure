  #declare MIR = MIR + 1;
 #end
 cylinder {
  <-LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2-LG_CORNER_SPACE, 3*LG_BRICK_WIDTH/2, -6*LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
  <-LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2-LG_CORNER_SPACE, -3*LG_BRICK_WIDTH/2, -6*LG_PLATE_HEIGHT+LG_CORNER_SPACE>,
