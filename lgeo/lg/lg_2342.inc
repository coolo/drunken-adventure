   <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, 0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT-LG_CORNER_SPACE-LG_E>,
   0.12-LG_CORNER_SPACE
  }
  torus {
   0.12-LG_CORNER_SPACE,
   LG_CORNER_SPACE
   rotate <90, 0, 0>
   translate <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, 0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT-LG_CORNER_SPACE>
  }
  cylinder {
   <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, -0.2, LG_BRICK_WIDTH/2-LG_E>,
   <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, -0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT-LG_CORNER_SPACE>,
   0.12
  }
  cylinder {
   <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, -0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT>,
   <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, -0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT-LG_CORNER_SPACE-LG_E>,
   0.12-LG_CORNER_SPACE
  }
  torus {
   0.12-LG_CORNER_SPACE,
   LG_CORNER_SPACE
   rotate <90, 0, 0>
   translate <LG_BRICK_WIDTH/2+LG_WALL_WIDTH/2+0.12, -0.2, LG_BRICK_WIDTH/2+LG_KNOB_HEIGHT-LG_CORNER_SPACE>
  }
  rotate <0, lg_angle1*180/pi, 0>
  translate <0, 0, LG_BRICK_HEIGHT>
 }
